<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>QA_Specialist_Percent_without_Burn__c</fullName>
    <formula>IF (QA_Specialist_Scorecard_Points_Earned__c &gt; 0, QA_Specialist_Scorecard_Points_Earned__c  / (  QA_Specialist_Scorecard_Points_Earned__c  + QA_Specialist_Scorecard_Points_From_Burn__c + QA_Specialist_Scorecard_Points_Missed__c), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>QA Specialist Percent without Burn</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
