<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Work_Time_Exceeded__c</fullName>
    <description>True if Case_age - Days to Assign - Days on External Hold - Days on internal Hold &gt; SLA Days</description>
    <formula>IF(Case_Age__c -  Days_to_Assign_Case__c -  Days_on_External_Hold__c -   Days_On_Internal_Hold__c  &gt; SLA_Days__c,True,False)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Work Time Exceeded</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
