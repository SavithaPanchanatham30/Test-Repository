    ISBLANK([Opportunity].Broker_Agency__c ),
    NOT(ISBLANK([Opportunity].Account.Sales_Primary_Agency__c))
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
