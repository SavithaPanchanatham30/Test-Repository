    <description>This process will update agency on opportunity products</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR
(
	AND
	(
		ISCHANGED([Opportunity].Broker_Agency__c ), 
		NOT(ISBLANK([Opportunity].Broker_Agency__c ) ) ,
		[Opportunity].IsClosed = FALSE,
		NOT(ISNEW())
	),
	
	AND
	(
		ISCHANGED([Opportunity].StageName ), 
		NOT(ISNEW()),
		NOT(ISBLANK([Opportunity].Broker_Agency__c ) ) ,
		AND
		(
			OR
			(
				ISPICKVAL(PRIORVALUE([Opportunity].StageName ),&#39;Closed Won&#39;),
				ISPICKVAL(PRIORVALUE([Opportunity].StageName ),&#39;Closed Lost&#39;)
			),
			OR
			(
				ISPICKVAL([Opportunity].StageName ,&#39;New&#39;),
				ISPICKVAL([Opportunity].StageName ,&#39;In Process&#39;)
			)		
		)	
	),

	AND
	(
		ISCHANGED([Opportunity].Broker_Agency__c ), 
		NOT(ISBLANK(PRIORVALUE([Opportunity].Broker_Agency__c))),
		ISBLANK([Opportunity].Broker_Agency__c ),
		[Opportunity].IsClosed = FALSE,
		NOT(ISNEW())
	)	
)

</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>OR
(
	AND
	(
		ISCHANGED({!myVariable_current.Broker_Agency__c} ), 
		NOT(ISBLANK({!myVariable_current.Broker_Agency__c} ) ) ,
		{!myVariable_current.IsClosed} = FALSE,
		NOT(ISNEW())
	),
	
	AND
	(
		ISCHANGED({!myVariable_current.StageName} ), 
		NOT(ISNEW()),
		NOT(ISBLANK({!myVariable_current.Broker_Agency__c} ) ) ,
		AND
		(
			OR
			(
				ISPICKVAL(PRIORVALUE({!myVariable_current.StageName} ),&#39;Closed Won&#39;),
				ISPICKVAL(PRIORVALUE({!myVariable_current.StageName} ),&#39;Closed Lost&#39;)
			),
			OR
			(
				ISPICKVAL({!myVariable_current.StageName} ,&#39;New&#39;),
				ISPICKVAL({!myVariable_current.StageName} ,&#39;In Process&#39;)
			)		
		)	
	),

	AND
	(
		ISCHANGED({!myVariable_current.Broker_Agency__c} ), 
		NOT(ISBLANK(PRIORVALUE({!myVariable_current.Broker_Agency__c}))),
		ISBLANK({!myVariable_current.Broker_Agency__c} ),
		{!myVariable_current.IsClosed} = FALSE,
		NOT(ISNEW())
	)	
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND 
(
